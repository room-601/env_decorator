{
  "name": "env_decorator",
  "description": "show your current environment",
  "manifest_version": 3,
  "version": "0.1",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "./page/popup.html"
  }
}
